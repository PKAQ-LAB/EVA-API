<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.nerv.sys.web.dict.mapper.DictItemMapper">
  <resultMap id="BaseResultMap" type="dictItem" extends="common.resultMap"/>
  <!--根据mainId查询数据-->
  <select id="listItemByMainId" resultType="dictItem">
    select * from sys_dict_item where main_id=#{mainId} order by orders
  </select>
</mapper>